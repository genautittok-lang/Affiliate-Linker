# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Database (Railway provides this automatically when you add PostgreSQL)
DATABASE_URL=postgresql://user:password@host:5432/database

# AliExpress API
ALIEXPRESS_APP_KEY=your_aliexpress_app_key
ALIEXPRESS_APP_SECRET=your_aliexpress_app_secret
ALIEXPRESS_TRACKING_ID=your_aliexpress_tracking_id

# AI Integrations (Replit provides these, for Railway use OpenAI directly)
OPENAI_API_KEY=your_openai_api_key
# OR use Replit AI Integrations:
# AI_INTEGRATIONS_OPENAI_BASE_URL=
# AI_INTEGRATIONS_OPENAI_API_KEY=

# Port (Railway sets this automatically)
PORT=5000
